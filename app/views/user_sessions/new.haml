#dialog
  %h1 denachtdienst.org login
  
  -form_for @user_session, :url => user_session_path do |f|
    %ul
      %li
        =f.label :login
        =f.text_field :login, :class => 'text', :size => 20
      %li
        =f.label :password
        =f.password_field :password, :class => 'text'
      %li=f.submit 'Login'
